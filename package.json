{
  "name": "clearway",
  "description": "Clearway test job",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npm run lint lint:eslint",
    "lint:eslint": "eslint \"src/**/*.ts\"",
    "lint:eslint:fix": "eslint \"{src}/**/*.ts\" --fix",
    "lint:eslint:partial": "eslint",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:styles:fix": "stylelint --fix \"src/**/*.scss\"",
    "lint:styles:partial": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.7",
    "@angular/common": "^19.1.7",
    "@angular/compiler": "^19.1.7",
    "@angular/core": "^19.1.7",
    "@angular/forms": "^19.1.7",
    "@angular/platform-browser": "^19.1.7",
    "@angular/platform-browser-dynamic": "^19.1.7",
    "@angular/router": "^19.1.7",
    "@ngneat/until-destroy": "^10.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.8",
    "@angular/cli": "^19.1.8",
    "@angular/compiler-cli": "^19.1.7",
    "@eslint/compat": "^1.2.7",
    "@stylistic/eslint-plugin-ts": "^4.0.1",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "^19.1.0",
    "eslint": "^9.21.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-rxjs": "^5.0.3",
    "husky": "^9.1.6",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.10",
    "stylelint": "^16.10.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.8.1",
    "typescript": "~5.5.2",
    "typescript-eslint": "^8.25.0"
  }
}
